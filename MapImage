 //create new hotspot set attributes as hotspot to map (picture)
        RectangleHotSpot spot1 = new RectangleHotSpot();
        spot1.HotSpotMode = HotSpotMode.Navigate;
        spot1.NavigateUrl = "page1.aspx";
        spot1.Left = 32;
        spot1.Right = 158;
        spot1.Top = 284;
        spot1.Bottom = 84;
        map.HotSpots.Add(spot1);


